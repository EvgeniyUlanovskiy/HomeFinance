<?xml version="1.0" encoding="UTF-8"?>

<?package view;?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TabPane fx:controller="controller.Controller" prefHeight="316.0" prefWidth="301.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <tabs>
        <Tab text="Ввод данных">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="330.0" prefWidth="314.0">
                    <children>
                        <TextField fx:id="inputField" alignment="CENTER" layoutX="76.0" layoutY="60.0" prefHeight="21.0" prefWidth="149.0" promptText="Введите сумму" AnchorPane.leftAnchor="76.0" AnchorPane.rightAnchor="76.0" AnchorPane.topAnchor="60.0">
                     <padding>
                        <Insets left="3.0" right="3.0" />
                     </padding></TextField>
                        <Label fx:id="currentBalanceTitle" alignment="CENTER" layoutX="18.0" layoutY="19.0" text="Ваш текущий баланс:" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS" />
